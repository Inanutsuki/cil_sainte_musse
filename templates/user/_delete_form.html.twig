<form method="post" action="{{ path('admin_user_delete', {'id': app.user.id}) }}" onsubmit="return confirm('Etes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_method" value="Supprimer">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ app.user.id) }}">
    <button class="btn">Delete</button>
</form>
