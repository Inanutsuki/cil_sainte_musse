<form method="post" action="{{ path('document_upload_delete', {'id': document_upload.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir le supprimer ?');">
    <input type="hidden" name="_method" value="Supprimer">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ document_upload.id) }}">
    <button class="btn">Supprimer</button>
</form>
